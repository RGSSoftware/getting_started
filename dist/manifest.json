{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "browser_action": {
  "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
    "http://*/*",
    "https://*/*"
    ],
      "js": ["bundle.js"]
    }
  ]
}
